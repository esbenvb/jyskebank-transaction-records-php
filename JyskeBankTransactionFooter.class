<?php
define('JYSKEBANK_FOOTER_TYPE', 'IB999999999999');

class JyskeBankTransactionFooter implements JyskeBankRecord {
  
  private $total;
  private $number;

  public function __construct($total, $number, $date = NULL) {
    $this->type = JYSKEBANK_FOOTER_TYPE;
    $this->number = $number;
    $this->total = $total;
    parent::__construct($date);
  }
}